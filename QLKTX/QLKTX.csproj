    <Content Include="Dataset\ds_SummaryVoucher.xss" />
    <Content Include="Dataset\ds_VoucherDetail.xsc" />
    <Content Include="Dataset\ds_VoucherDetail.xss" />
    <Content Include="Dataset\ds_VoucherDReasonReport.xsc" />
    <Content Include="Dataset\ds_VoucherDReasonReport.xss" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="wwwroot\Reports\rpt_StatisticsOfReceiptsReport.rdlc" />
    <EmbeddedResource Include="wwwroot\Reports\rpt_SummaryVoucherReport.rdlc" />
    <EmbeddedResource Include="wwwroot\Reports\rpt_VoucherDetailReport.rdlc" />
    <EmbeddedResource Include="wwwroot\Reports\rpt_VoucherDReasonReport.rdlc" />
