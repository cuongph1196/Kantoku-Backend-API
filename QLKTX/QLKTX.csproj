<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <!--<Nullable>enable</Nullable>-->
    <ImplicitUsings>enable</ImplicitUsings>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <!--<EnablePreviewFeatures>true</EnablePreviewFeatures>-->
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\Reports\rpt_StatisticsOfReceiptsReport.rdlc" />
    <Content Remove="wwwroot\Reports\rpt_SummaryVoucherReport.rdlc" />
    <Content Remove="wwwroot\Reports\rpt_VoucherDetailReport.rdlc" />
    <Content Remove="wwwroot\Reports\rpt_VoucherDReasonReport.rdlc" />
    <Content Remove="wwwroot\rpt_StatisticsOfPaymentsReport.rdlc" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Dataset\ds_StatisticsOfPayments.xsc" />
    <None Remove="Dataset\ds_StatisticsOfPayments.xss" />
    <None Remove="Dataset\ds_StatisticsOfReceipts.xsc" />
    <None Remove="Dataset\ds_StatisticsOfReceipts.xss" />
    <None Remove="Dataset\ds_SummaryVoucher - Copy.xsc" />
    <None Remove="Dataset\ds_SummaryVoucher - Copy.xss" />
    <None Remove="Dataset\ds_SummaryVoucherReport.xsc" />
    <None Remove="Dataset\ds_SummaryVoucherReport.xss" />
    <None Remove="Dataset\ds_VoucherDetail - Copy.xsc" />
    <None Remove="Dataset\ds_VoucherDetail - Copy.xss" />
    <None Remove="Dataset\ds_VoucherDetail.xsc" />
    <None Remove="Dataset\ds_VoucherDetail.xss" />
    <None Remove="Dataset\ds_VoucherDReasonReport.xsc" />
    <None Remove="Dataset\ds_VoucherDReasonReport.xss" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Dataset\ds_StatisticsOfPayments.xsc" />
    <Content Include="Dataset\ds_StatisticsOfPayments.xss" />
